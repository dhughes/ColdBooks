			<xsd:simpleType>
				<xsd:restriction base="STRTYPE">
					<xsd:maxLength value="9"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:element>
		<xsd:element name="CommercialCardCode" minOccurs="0">
			<xsd:simpleType>
				<xsd:restriction base="STRTYPE">
					<xsd:maxLength value="25"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:element>
		<xsd:element ref="SalesTaxAmount" minOccurs="0"/>
		<xsd:element name="CardSecurityCode" minOccurs="0">
			<xsd:simpleType>
				<xsd:restriction base="STRTYPE">
					<xsd:maxLength value="4"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:element>
		<xsd:element ref="Lodging" minOccurs="0"/>
		<xsd:element ref="Restaurant" minOccurs="0"/>
		<xsd:element name="BatchID" minOccurs="0">
			<xsd:simpleType>
				<xsd:restriction base="STRTYPE">
					<xsd:maxLength value="4"/>
					<xsd:minLength value="4"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:element>
	</xsd:sequence>
	<xsd:attribute name="requestID" type="STRTYPE"/>
</xsd:complexType>

